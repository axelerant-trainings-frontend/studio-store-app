{#
/*
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region <div>.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}

{% if content %}
<footer class='footer-wrapper' role="contentinfo" aria-label="Footer">
  <div class='footer-wrapper__details'>
    {{ elements.footerdetails }}
    {{ elements.footerlinks }}
  </div>
  <nav class='footer-wrapper__menu' aria-label="Footer Menu">
    {{ elements.whatwedo }}
    {{ elements.company }}
    {{ elements.support }}
    {{ elements.contact }}
  </nav>
</footer>
{% endif %}

